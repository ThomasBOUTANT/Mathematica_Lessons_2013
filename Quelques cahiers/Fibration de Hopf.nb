Cell[BoxData[
 RowBox[{
  RowBox[{"fibre", "[", 
   RowBox[{"\[Alpha]_", ",", "\[Beta]_", ",", "\[Phi]_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
