Cell[TextData[{
 "Soit ",
 Cell[BoxData[
  FormBox[
   RowBox[{"M", "=", 
    RowBox[{"(", 
     RowBox[{"x", ",", "y", ",", "z"}], ")"}]}], TraditionalForm]]],
 "  un point de la sph\[EGrave]re unit\[EAcute] ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["S", "2"], TraditionalForm]]],
 ". Soit ",
 Cell[BoxData[
  FormBox[Cell[TextData[Cell[BoxData[
    FormBox[
     RowBox[{"N", " ", "=", " ", 
      RowBox[{"(", 
       RowBox[{"0", ",", " ", "0", ",", " ", "1"}], ")"}]}], 
     TraditionalForm]]]]], TraditionalForm]]],
 " le p\[OHat]le Nord de cette m\[EHat]me sph\[EGrave]re. Lorsque ",
 Cell[BoxData[
  FormBox[
   RowBox[{"M", "\[NotEqual]", "N"}], TraditionalForm]]],
 ", la droite ",
 Cell[BoxData[
  FormBox[
   RowBox[{"(", 
    RowBox[{"N", " ", "M"}], ")"}], TraditionalForm]]],
 " a pour \[EAcute]quations param\[EAcute]triques ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"X", "=", 
     RowBox[{"\[Lambda]", " ", "x"}]}], ",", " ", 
    RowBox[{"Y", "=", 
     RowBox[{"\[Lambda]", " ", "y"}]}], ",", 
    RowBox[{"Z", "=", 
     RowBox[{"1", "+", 
      RowBox[{"\[Lambda]", "(", 
       RowBox[{"z", "-", "1"}], ")"}]}]}]}], TraditionalForm]]],
 ". Elle intersecte le plan ",
 Cell[BoxData[
  FormBox[
   RowBox[{"x", " ", "O", " ", "y"}], TraditionalForm]]],
 " lorsque ",
 Cell[BoxData[
  FormBox[
   RowBox[{"Z", "=", "0"}], TraditionalForm]]],
 ", c'est \[AGrave] dire lorsque ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Lambda]", "=", 
    FractionBox["1", 
     RowBox[{"1", "-", "z"}]]}], TraditionalForm]]],
 ". Le point ",
 Cell[BoxData[
  FormBox[
   RowBox[{"P", "=", 
    RowBox[{"(", 
     RowBox[{
      FractionBox["x", 
       RowBox[{"1", "-", "z"}]], ",", 
      FractionBox["y", 
       RowBox[{"1", "-", "z"}]]}], ")"}]}], TraditionalForm]]],
 " est la ",
 StyleBox["projection st\[EAcute]r\[EAcute]ographique",
  FontSlant->"Italic"],
 " du point ",
 Cell[BoxData[
  FormBox["M", TraditionalForm]]],
 " sur le plan ",
 Cell[BoxData[
  FormBox[
   RowBox[{"x", " ", "O", " ", "y"}], TraditionalForm]]],
 "."
}], "Text",
 CellChangeTimes->{{3.450402419139735*^9, 3.450402699643735*^9}, {
