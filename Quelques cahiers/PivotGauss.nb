fois plus lente que celle que nous allons \[EAcute]crire)."
}], "Text"],

Cell[BoxData[
 RowBox[{
  RowBox[{"pivoter", "[", 
   RowBox[{"m_", ",", "k_"}], "]"}], ":=", "\[IndentingNewLine]", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"a", "=", "m"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{"a", "[", 
       RowBox[{"[", "k", "]"}], "]"}], "=", 
      RowBox[{
       RowBox[{"a", "[", 
        RowBox[{"[", "k", "]"}], "]"}], "/", 
       RowBox[{"a", "[", 
        RowBox[{"[", 
         RowBox[{"k", ",", "k"}], "]"}], "]"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Do", "[", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{"i", "\[NotEqual]", "k"}], ",", 
         RowBox[{
          RowBox[{"a", "[", 
           RowBox[{"[", "i", "]"}], "]"}], "=", 
          RowBox[{
           RowBox[{"a", "[", 
            RowBox[{"[", "i", "]"}], "]"}], "-", 
           RowBox[{
            RowBox[{"a", "[", 
             RowBox[{"[", 
              RowBox[{"i", ",", "k"}], "]"}], "]"}], "*", 
            RowBox[{"a", "[", 
             RowBox[{"[", "k", "]"}], "]"}]}]}]}]}], "]"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{"{", 
        RowBox[{"i", ",", "1", ",", 
         RowBox[{"Length", "[", "a", "]"}]}], "}"}]}], "\[IndentingNewLine]", 
      "]"}], ";", "\[IndentingNewLine]", "a"}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input"],

