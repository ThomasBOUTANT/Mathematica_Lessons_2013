Cell[TextData[{
 "La fonction ",
 StyleBox["pivoterSansRisque[m,p,q,n]", "Input"],
 " effectue une \[EAcute]tape de l'algorithme du pivot sur la matrice ",
 Cell[BoxData[
  FormBox["m", TraditionalForm]]],
 ". La recherche du pivot se fait en-dessous du coefficient ligne ",
 Cell[BoxData[
  FormBox["p", TraditionalForm]]],
 ", colonne ",
 Cell[BoxData[
  FormBox["q", TraditionalForm]]],
 ", et cette recherche ne d\[EAcute]passe pas la ligne ",
 Cell[BoxData[
  FormBox["n", TraditionalForm]]],
 " de la matrice. Au cas o\[UGrave] le pivot est nul, la fonction renvoie la \
matrice ",
 Cell[BoxData[
  FormBox["m", TraditionalForm]]],
 " inchang\[EAcute]e ainsi qu'un \"drapeau\" ",
 StyleBox["False", "Input"],
 ". Sinon, La fonction renvoie la matrice ",
 Cell[BoxData[
  FormBox["m", TraditionalForm]]],
 " modifi\[EAcute]e et un drapeau ",
 StyleBox["True", "Input"],
 "."
}], "Text"],

Cell[BoxData[
 RowBox[{
  RowBox[{"pivoterSansRisque", "[", 
   RowBox[{"m_", ",", "p_", ",", "q_", ",", "n_"}], "]"}], ":=", 
  "\[IndentingNewLine]", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"a", "=", "m"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"p1", "=", 
      RowBox[{"rechercherPivot", "[", 
       RowBox[{"a", ",", "p", ",", "q", ",", "n"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"a", "[", 
         RowBox[{"[", 
          RowBox[{"p1", ",", "q"}], "]"}], "]"}], "\[Equal]", "0"}], ",", 
       RowBox[{"{", 
        RowBox[{"a", ",", "False"}], "}"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"a", "=", 
         RowBox[{"echangerLignes", "[", 
          RowBox[{"a", ",", "p", ",", "p1"}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"a", "[", 
          RowBox[{"[", "p", "]"}], "]"}], "=", 
         RowBox[{
          RowBox[{"a", "[", 
           RowBox[{"[", "p", "]"}], "]"}], "/", 
          RowBox[{"a", "[", 
           RowBox[{"[", 
            RowBox[{"p", ",", "q"}], "]"}], "]"}]}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"Do", "[", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"If", "[", 
           RowBox[{
            RowBox[{"i", "\[NotEqual]", "p"}], ",", 
            RowBox[{
             RowBox[{"a", "[", 
              RowBox[{"[", "i", "]"}], "]"}], "=", 
             RowBox[{
              RowBox[{"a", "[", 
               RowBox[{"[", "i", "]"}], "]"}], "-", 
              RowBox[{
               RowBox[{"a", "[", 
                RowBox[{"[", 
                 RowBox[{"i", ",", "q"}], "]"}], "]"}], "*", 
               RowBox[{"a", "[", 
                RowBox[{"[", "p", "]"}], "]"}]}]}]}]}], "]"}], ",", 
          "\[IndentingNewLine]", 
          RowBox[{"{", 
           RowBox[{"i", ",", "1", ",", "n"}], "}"}]}], "\[IndentingNewLine]", 
         "]"}], ";", "\[IndentingNewLine]", 
        RowBox[{"{", 
         RowBox[{"a", ",", "True"}], "}"}]}]}], "\[IndentingNewLine]", 
      "]"}]}]}], "\[IndentingNewLine]", "]"}]}]], "Input"],

Cell[BoxData[
 RowBox[{
