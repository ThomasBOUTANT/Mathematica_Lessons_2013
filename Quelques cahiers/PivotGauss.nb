Cell[TextData[{
 "La fonction ",
 StyleBox["rechercherPivot[a,p,q,n]", "Input"],
 " recherche entre la ligne ",
 Cell[BoxData[
  FormBox["p", TraditionalForm]]],
 " et la ligne ",
 Cell[BoxData[
  FormBox["n", TraditionalForm]]],
 " de la matrice ",
 Cell[BoxData[
  FormBox["a", TraditionalForm]]],
 ", ceci sur la colonne ",
 Cell[BoxData[
  FormBox["q", TraditionalForm]]],
 ", la ligne contenant le plus grand coefficient en valeur absolue. Si un \
coefficient est non nul, le plus grand est \[EAcute]videmment un tr\[EGrave]s \
bon candidat. De plus, plus un pivot est grand, moins les erreurs de calculs \
sont importantes. \nLa fonction ",
 StyleBox["rechercherPivot", "Input"],
 " renvoie l'indice de la ligne correspondant au pivot qui nous \
int\[EAcute]resse."
}], "Text"],

Cell[BoxData[
 RowBox[{
  RowBox[{"rechercherPivot", "[", 
   RowBox[{"a_", ",", "p_", ",", "q_", ",", "n_"}], "]"}], ":=", 
  "\[IndentingNewLine]", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"imax", ",", "k"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"imax", "=", "p"}], ";", "\[IndentingNewLine]", 
     RowBox[{"Do", "[", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"Abs", "[", 
           RowBox[{"a", "[", 
            RowBox[{"[", 
             RowBox[{"k", ",", "q"}], "]"}], "]"}], "]"}], ">", 
          RowBox[{"Abs", "[", 
           RowBox[{"a", "[", 
            RowBox[{"[", 
             RowBox[{"imax", ",", "q"}], "]"}], "]"}], "]"}]}], ",", 
         RowBox[{"imax", "=", "k"}]}], "]"}], ",", "\[IndentingNewLine]", 
       RowBox[{"{", 
        RowBox[{"k", ",", "p", ",", "n"}], "}"}]}], "\[IndentingNewLine]", 
      "]"}], ";", "\[IndentingNewLine]", "imax"}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input"],

