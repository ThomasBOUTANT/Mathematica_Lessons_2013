Cell[BoxData[
 RowBox[{"Kn", "=", 
  RowBox[{
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      SubscriptBox["T", "0"], "-", 
      SubscriptBox["T", "1"]}], ")"}], "/", "L"}], "*", 
   RowBox[{"(", 
    RowBox[{
     RowBox[{"-", 
      RowBox[{
       SubsuperscriptBox["\[Integral]", 
        RowBox[{"-", "L"}], 
        RowBox[{
         RowBox[{"-", "L"}], "/", "2"}]], 
       RowBox[{
        RowBox[{"Cos", "[", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{
            RowBox[{"2", "*", "n"}], "+", "1"}], ")"}], "*", "\[Pi]", "*", 
          RowBox[{"x", "/", "L"}]}], "]"}], 
        RowBox[{"\[DifferentialD]", "x"}]}]}]}], "+", 
     RowBox[{
      SubsuperscriptBox["\[Integral]", 
       RowBox[{
        RowBox[{"-", "L"}], "/", "2"}], 
       RowBox[{"L", "/", "2"}]], 
      RowBox[{
       RowBox[{"Cos", "[", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{
           RowBox[{"2", "*", "n"}], "+", "1"}], ")"}], "*", "\[Pi]", "*", 
         RowBox[{"x", "/", "L"}]}], "]"}], 
       RowBox[{"\[DifferentialD]", "x"}]}]}], "-", 
     RowBox[{
      SubsuperscriptBox["\[Integral]", 
       RowBox[{"L", "/", "2"}], "L"], 
      RowBox[{
       RowBox[{"Cos", "[", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{
           RowBox[{"2", "*", "n"}], "+", "1"}], ")"}], "*", "\[Pi]", "*", 
         RowBox[{"x", "/", "L"}]}], "]"}], 
       RowBox[{"\[DifferentialD]", "x"}]}]}]}], ")"}]}]}]], "Input",
 PageWidth->WindowWidth],

